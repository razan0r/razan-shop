          {data.subImages.map((img,index)=>
          <React.Fragment  key={index}>
                    <ReactImageMagnify {...{
              smallImage: {
                  alt: 'Wristwatch by Ted Baker London',
                  isFluidWidth: true,
                  src: img.secure_url,
              },
              largeImage: {
                  src: img.secure_url,
                  width: 1200,
                  height: 1800
              },enlargedImageContainerDimensions:{
                width: 900,
                height: 900,
              },
              isHintEnabled:true,
          }} />
          </React.Fragment>
          )}